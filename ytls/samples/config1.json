{
  "application": {
    "name": "SampleApp",
    "version": "1.1.0",
    "debug": false,
    "features": {
      "authentication": {
        "enabled": true,
        "methods": [
          "password",
          "oauth",
          "biometric"
        ]
      },
      "analytics": {
        "enabled": true,
        "provider": "GoogleAnalytics"
      }
    },
    "database": {
      "host": "db.example.com",
      "port": 5432,
      "username": "admin",
      "password": "supersecret",
      "options": {
        "pool_size": 25,
        "timeout": 30,
        "ssl": true
      }
    },
    "servers": [
      {
        "name": "server1",
        "ip": "192.168.1.10",
        "roles": [
          "web",
          "api",
          "cache"
        ]
      },
      {
        "name": "server3",
        "ip": "192.168.1.12",
        "roles": [
          "proxy"
        ]
      }
    ]
  }
}